  position: relative;
  width: 100%;
  border-radius: 10px;
  margin-top: 140rpx;
  z-index: 9;
}

.Geography {
  position: absolute;
  z-index: 9;
  top: 12.7rpx;
  left: 31.4rpx;
}

.Geography image {
  width: 20.53rpx;
  height: 26rpx;
  margin-right: 20rpx;
}

.Geography text {
  font-size: 22rpx;
}

.demo-text-1 {
  border-radius: 10px;
