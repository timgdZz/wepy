  height: 85rpx;
  font-size: 24rpx;
  margin-bottom: 36rpx;
  text-align: center;
 
}
.iconBox image {
  width: 48rpx;
  height: 48rpx;
}
.serve {
  width: 100%;
  min-height: 200rpx;
  border: #c4c4c4 2rpx solid;
  border-radius: 30rpx;
  background-color: #fff;
}
.tabbar {
  display: flex;
