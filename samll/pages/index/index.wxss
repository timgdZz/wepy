.banner {
  width: 100%;
  height: 350rpx;
  background-color: #f6f6f6;
  padding-top: 61.6rpx;
  box-sizing: border-box;
}

.bannerBox {
  height: 100%;
  height: auto;
  display: flex;
  display: -webkit-flex;
  justify-content: space-between;
  flex-direction: row;
  flex-wrap: wrap;
}

